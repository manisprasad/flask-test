{
  "manifest_version": 3,
  "name": "Grammerly",
  "description": "Chrome Setup.",
  "version": "1.0.0",
   "permissions": ["clipboardWrite", "scripting", "activeTab", "localStorage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
